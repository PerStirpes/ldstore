  "version": "0.1.0",
  "license": "MIT",
  "author": "You",
  "dependencies": {
    "@emotion/core": "10.0.22",
    "@emotion/styled": "10.0.23",
    "apollo-boost": "0.3.1",
    "emotion": "10.0.23",
    "emotion-server": "10.0.17",
    "eslint-config-prettier": "4.1.0",
    "gatsby": "2.18.7",
    "gatsby-cli": "2.8.16",
    "gatsby-image": "2.2.33",
    "gatsby-plugin-emotion": "4.1.16",
    "gatsby-plugin-google-analytics": "2.1.29",
    "gatsby-plugin-layout": "1.1.15",
    "gatsby-plugin-manifest": "2.2.31",
    "gatsby-plugin-offline": "2.0.24",
    "gatsby-plugin-react-helmet": "3.1.16",
    "gatsby-plugin-sharp": "2.3.5",
    "gatsby-source-shopify": "^3.0.32",
    "gatsby-transformer-sharp": "^2.3.7",
    "launchdarkly-react-client-sdk": "2.18.0",
    "react": "^16.12.0",
    "react-apollo": "3.1.3",
    "react-dom": "^16.12.0",
    "react-helmet": "5.2.1",
    "react-icons": "3.8.0",
    "react-onclickoutside": "6.9.0",
    "react-router-dom": "5.1.2",
    "recompose": "0.30.0",
    "shopify-buy": "2.8.1"
  },
  "keywords": [
    "gatsby"
  ],
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --ignore-path .gitignore --ignore-path .prettierignore --fix",
      "git add"
    ],
    "*.{md,css,scss,yaml,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged npm test || node on-lint-error.js",
      "prepublish": "npm test",
      "prepush": "npm test",
      "test": "npm run lint"
    }
  },
  "scripts": {
    "build": "gatsby build",
    "serve": "gatsby serve",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.js",
    "lint": "eslint .",
    "start": "npm run develop",
    "test": "npm run stylelint",
    "stylelint": "stylelint './src/**/*.js'",
    "now-build": "yarn build"
  },
  "devDependencies": {
    "eslint-plugin-emotion": "^10.0.7",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^2.0.0",
    "stylelint-processor-styled-components": "^1.5.2"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/PerStripes/store.git"
  },
  "bugs": {
    "url": "https://github.com/PerStripes/store/issues"
  },
  "homepage": "https://github.com/PerStripes/store#readme",
  "main": "gatsby-browser.js"
}
