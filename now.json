{
  "version": 2,
  "public": true,
  "alias": "shop-darkly.now.sh",
  "github": {
    "autoAlias": true
  },
  "scope": "freeagent",
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "status": 404, "dest": "public/404/index.html" },
    {
      "src": "^/public/static/(.*)",
      "headers": { "cache-control": "public,max-age=31536000,immutable" }
    },
    {
      "src": "^/(.*).(css|js)",
      "headers": { "cache-control": "public,max-age=31536000,immutable" }
    },
    {
      "src": "^/(.*).html",
      "headers": { "cache-control": "public,max-age=0,must-revalidate" }
    }
  ]
}
